    "@vitest/coverage-v8": "^2.1.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.17.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-format": "^2.0.1",
